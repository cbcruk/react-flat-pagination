{
  "name": "react-flat-pagination",
  "version": "0.0.3",
  "description": "말줄임표 없는 React 페이지네이션 컴포넌트",
  "keywords": [
    "react",
    "pagination",
    "flat",
    "headless"
  ],
  "license": "MIT",
  "author": "cbcruk",
  "homepage": "https://cbcruk.github.io/react-flat-pagination/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbcruk/react-flat-pagination.git"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --format esm,cjs --dts --legacy-output --watch",
    "build": "tsup --format esm,cjs --dts --legacy-output",
    "release": "pnpm build && pnpm publish --access=public",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "test": "vitest",
    "ladle": "ladle serve --port 4123",
    "ladle:build": "ladle build"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@ladle/react": "^5.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/ui": "^0.24.5",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.31.10",
    "jsdom": "^20.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.24.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "stackblitz": {
    "startCommand": "npm run test"
  }
}
